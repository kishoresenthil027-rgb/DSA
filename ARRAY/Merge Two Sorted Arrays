#include <stdio.h>

int main() {
    int n1, n2;
    int arr1[100], arr2[100], merged[200];
    int i = 0, j = 0, k = 0;

    // Read first array size and elements
    printf("Enter number of elements in first sorted array: ");
    scanf("%d", &n1);

    printf("Enter elements of first sorted array:\n");
    for (i = 0; i < n1; i++) {
        scanf("%d", &arr1[i]);
    }

    // Read second array size and elements
    printf("Enter number of elements in second sorted array: ");
    scanf("%d", &n2);

    printf("Enter elements of second sorted array:\n");
    for (j = 0; j < n2; j++) {
        scanf("%d", &arr2[j]);
    }

    i = 0;
    j = 0;

    // Merge both arrays
    while (i < n1 && j < n2) {
        if (arr1[i] <= arr2[j]) {
            merged[k++] = arr1[i++];
        } else {
            merged[k++] = arr2[j++];
        }
    }

    // Copy remaining elements of first array
    while (i < n1) {
        merged[k++] = arr1[i++];
    }

    // Copy remaining elements of second array
    while (j < n2) {
        merged[k++] = arr2[j++];
    }

    // Display merged array
    printf("Merged sorted array:\n");
    for (i = 0; i < k; i++) {
        printf("%d ", merged[i]);
    }

    return 0;
}
