#include <stdio.h>

int main() {
    int n, target;
    int arr[100];
    int found = 0;

    // Input array size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input array elements
    printf("Enter array elements:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Input target value
    printf("Enter target value: ");
    scanf("%d", &target);

    // Check for two sum
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] + arr[j] == target) {
                printf("Two numbers found: %d + %d = %d\n",
                       arr[i], arr[j], target);
                found = 1;
                break;
            }
        }
        if (found)
            break;
    }

    // If no pair found
    if (!found) {
        printf("No two numbers add up to the target value.\n");
    }

    return 0;
}
