#include <stdio.h>

int main() {
    int n;
    int arr[100];
    int pos = 0;

    // Read number of elements
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Read array elements
    printf("Enter array elements:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Move non-zero elements to the front
    for (int i = 0; i < n; i++) {
        if (arr[i] != 0) {
            arr[pos] = arr[i];
            pos++;
        }
    }

    // Fill remaining positions with zero
    while (pos < n) {
        arr[pos] = 0;
        pos++;
    }

    // Display the result
    printf("Array after moving zeroes:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
